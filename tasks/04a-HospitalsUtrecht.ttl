@prefix : <https://github.com/quangis/transformation-algebra#>.
@prefix from: <https://github.com/quangis/transformation-algebra#from>.
@prefix type: <https://github.com/quangis/cct#type>.
@prefix via: <https://github.com/quangis/cct#operator>.

_:HospitalPoints
    type: "R(Obj, Reg * Nom)".
_:IncidentLocations
    type: "R(Obj, Reg * Nom)".

[] a :Query;
    :question "What is the travel distance to the nearest hospital in California?";
    :workflow <https://example.com/#HospitalsUtrecht_Network>;
    :workflow <https://example.com/#HospitalsUtrecht_Near>;
    :input _:HospitalPoints, _:IncidentLocations;
    :output [
        type: "R(Obj, Reg * Ratio)";
        from: [
            type: "R3(Obj, Ratio, Obj)"; # TODO: deprecate R3
            from: _:HospitalPoints;
            from: _:IncidentLocations
        ]
    ].
