@prefix : <https://github.com/quangis/transformation-algebra#>.
@prefix from: <https://github.com/quangis/transformation-algebra#from>.
@prefix type: <https://github.com/quangis/cct#type>.
@prefix via: <https://github.com/quangis/cct#operator>.

_:HospitalPoints
    type: "R(Obj, Reg * Nom)".
_:IncidentLocations
    type: "R(Obj, Reg * Nom)".

[] a :Task;
    :question "What is the travel distance to the nearest hospital in California?";
    :implementation <https://example.com/#HospitalsUtrecht_Network>;
    :implementation <https://example.com/#HospitalsUtrecht_Near>;
    :input _:HospitalPoints, _:IncidentLocations;
    :output [
        # Minimize distance matrix over objects (TODO technical?)
        type: "R(Obj, Reg * Ratio)"; # A
        from: [
            # Generate a distance matrix from events (as objects) to hospital objects
            type: "R3(Obj, Ratio, Obj)"; # B TODO: deprecate R3
            from: _:HospitalPoints; # D
            from: _:IncidentLocations # C
        ]
    ].
