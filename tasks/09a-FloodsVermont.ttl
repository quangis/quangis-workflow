        type: "R(Ord, Ratio)";
        from: [
            # type: "R(Ord, Reg)";
            # from: [
                type: "R(Loc, Ratio)";
                from: [
                    type: "R3(Loc, Ratio, Loc)"; # TODO: deprecate R3
                    from: [
                        type: "R(Loc, Itv)" # DEM
                    ]
                ], [
                    type: "R(Nom, Reg)";
                    from: [
                        type: "R(Obj, Reg * Nom)" # pour point
                    ]
                ]
            ]
        # ]
    ].
